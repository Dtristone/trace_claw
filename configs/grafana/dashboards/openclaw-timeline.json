{
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "OpenClaw Model Latency (ms)",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "rate(openclaw_run_duration_ms_sum[5m]) / rate(openclaw_run_duration_ms_count[5m])",
          "legendFormat": "avg latency ({{openclaw_model}})"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        },
        "overrides": []
      }
    },
    {
      "title": "Token Usage",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "rate(openclaw_tokens_total[5m])",
          "legendFormat": "{{openclaw_token}} ({{openclaw_model}})"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": []
      }
    },
    {
      "title": "CPU Usage (%)",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "system_cpu_usage_percent{cpu=\"total\"}",
          "legendFormat": "CPU total"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "max": 100
        },
        "overrides": []
      }
    },
    {
      "title": "Memory Usage (%)",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 8 },
      "targets": [
        {
          "expr": "system_memory_usage_percent",
          "legendFormat": "Memory %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "max": 100
        },
        "overrides": []
      }
    },
    {
      "title": "Network I/O Rate (bytes/s)",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 8 },
      "targets": [
        {
          "expr": "system_network_bytes_sent_rate",
          "legendFormat": "sent ({{interface}})"
        },
        {
          "expr": "system_network_bytes_recv_rate",
          "legendFormat": "recv ({{interface}})"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "Bps"
        },
        "overrides": []
      }
    },
    {
      "title": "Model Cost (USD)",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 16 },
      "targets": [
        {
          "expr": "sum(openclaw_cost_usd_total)",
          "legendFormat": "Total Cost"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD"
        },
        "overrides": []
      }
    },
    {
      "title": "Error Rate",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 16 },
      "targets": [
        {
          "expr": "sum(rate(openclaw_webhook_error_total[5m])) / sum(rate(openclaw_webhook_received_total[5m]))",
          "legendFormat": "Error Rate"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit"
        },
        "overrides": []
      }
    },
    {
      "title": "Message Processing Duration",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "h": 4, "w": 12, "x": 12, "y": 16 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(openclaw_message_duration_ms_bucket[5m]))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.50, rate(openclaw_message_duration_ms_bucket[5m]))",
          "legendFormat": "p50"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        },
        "overrides": []
      }
    },
    {
      "title": "Queue Depth",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 20 },
      "targets": [
        {
          "expr": "openclaw_queue_depth",
          "legendFormat": "{{openclaw_lane}}"
        }
      ]
    },
    {
      "title": "Session States",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 20 },
      "targets": [
        {
          "expr": "rate(openclaw_session_state_total[5m])",
          "legendFormat": "{{openclaw_state}}"
        }
      ]
    }
  ],
  "schemaVersion": 39,
  "tags": ["openclaw", "trace_claw", "ai"],
  "templating": {
    "list": []
  },
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "title": "OpenClaw Trace Dashboard",
  "uid": "openclaw-trace-timeline"
}
